pipeline: bronze_load
flow: customer_bronze

- actions:
  - name: load_customer_table
    type: load
    operational_metadata: ["_ingestion_timestamp","_source_file_size","_record_hash"]
    readMode: batch
    source:
      type: delta
      database: "{catalog}.{bronze_schema}"
      table: lineitem
    target: v_lineitem_raw_table
    description: "Load lineitem table from bronze schema" 